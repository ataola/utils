{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [">0.25%", "not ie 11", "not op_mini all"]
        },
        "exclude": ["@babel/plugin-transform-async-to-generator", "@babel/plugin-transform-arrow-functions", "@babel/plugin-proposal-class-properties"],
        "corejs": "2",
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": ["istanbul", "@babel/transform-runtime"]
}
